{
  "name": "vue3-easy-data-table",
  "author": "HC200ok",
  "description": "A customizable and easy-to-use data table component made with Vue.js 3.x.",
  "private": false,
  "version": "1.5.16",
  "types": "./types/main.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "vue3",
    "data-table-component",
    "data-table",
    "vue3-component",
    "vue-component"
  ],
  "homepage": "https://github.com/HC200ok/vue3-easy-data-table",
  "main": "./dist/vue3-easy-data-table.umd.js",
  "module": "./dist/vue3-easy-data-table.es.js",
  "exports": {
    ".": {
      "types": "./types/main.d.ts",
      "import": "./dist/vue3-easy-data-table.es.js",
      "require": "./dist/vue3-easy-data-table.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "cross-env VITE_APP_RUN_BUNDLE_ANALYZER=true npm run build",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.html,.js,.ts --fix src",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "uuid": "^9.0.0",
    "vue": "^3.2.25",
    "vue-fa": "^4.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.29",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.51.0",
    "typescript": "^4.5.4",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue-eslint-parser": "^9.0.1",
    "vue-tsc": "^0.34.7"
  }
